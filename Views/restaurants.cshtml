<h2>All Restaurants</h2>
@if (Model.Count == 0)
{
  <p>There are no restaurants yet.</p>
}
<ul>
  @foreach (var restaurant in Model)
  {
    <li><a href="/restaurant/@restaurant.GetId()">@restaurant.GetRestaurantName()</li>
  }
</ul>
<a href="/restaurant/new">Add a new restaurant</a>
<a href="/">Back to home</a>
<a href="/restaurant/all"></a>
