<h1>The Best Restaurants in Portland</h1>

@if (Model.Count == 0)
{
  <p>No cuisine types have been added yet!</p>
}

<ul>
  @foreach (var cuisine in Model)
  {
    <li>@cuisine.GetCuisineName()</li>
  }
</ul>

<p><a href="/restaurant/new">Add a new restaurant</a></p>
<a href="/cuisine/new">Add a new cuisine</a>
